          "_customCSS": "\n\n.medarbeidere {\n  width: 100%;\nheight: 435px;\n    background-color: white;\n}\n\n.konteiner{\n    position: relative;\n    height: 100%;\n    width: 100%;\n    margin: auto;\n    width: 100%;\n    \n}\n\n.rad{\n    position:relative;\n    top:25%;\n    height: 17.5vw;\n    width: 100%;\n}\n\n.galleri{\n    position:relative;\n    height: 100%;\n    width: 39vw;\n    text-align: center;\n    float:left;\n    margin: 0px 0vh 0px 12vh;\n    border: 1px solid black;\n    border-radius:1%;\n    \n}\n.img-box{\n    height:90%;\n    margin-top: 4.4vh;\n    margin-left: 5px;\n    border-radius: 1%;\n    float:left; \n}\n.box{\n    height:100%;\n    //margin-top: 3.5vh;\n    margin-left: 5px;\n    border-radius: 1%;\n    float:left;\n    //border:solid 1px;\n    //position:fixed;\n}\n\n.img-box img{\n    height:auto;\n    border-radius: 2.5%;\n    float:left;  \n    width:15.2vw;\n    position:absolute;\n   bottom:0px;\n}\n.box img{\n    height:auto;\n    border-radius: 2.5%;\n    float:left;  \n    width:15.2vw;\n    position:absolute;\n   bottom:0px;\n}\n\n.overlay{\n    position:relative;\n    opacity: 1;\n    width:100%;\n    height:100%;\n    -webkit-transition: 1s; /* For Safari 3.1 to 6.0 */\n    transition: 1s;\n    border-radius:2.5%;\n    \n}\n\n.tekst{\n    font-family: 'Lora', serif;\n    position: absolute;\n    //top:6vh;\n    //left:55%;\n    bottom:0px;\n    right:0px;\n    //transform: translate(-50%, -50%);\n    width: 45%;\n    height:80%;\n    text-align: left;\n    line-height: 3.1vh + 3.1vw;\n    line-height: 260%;\n    opacity: 1;\n    //border:solid 1px;\n    z-index:2;\n    font-family: 'Lora';\n    font-size: 0.5vw + 0.5vh;\n}\n\n.name{\n    font-family: 'Great Vibes' ;\n    padding-left:7%;\n    font-size: 20px;\n    weight: bold;\n}\n\n@media screen and (max-width: 400px) {\n    .medarbeidere{\n        height:270px;\n    }\n    .galleri{\n        height:18vh;\n        width: 90vw;\n        margin: 0px 0vh 20px 2vh;\n    }\n    .galleri img{\n    height:auto;\n    width:29vw;\n    border-radius: 2.5%;\n    float:left;  \n}\n    .img-box{\n    height:90%;\n    margin-top: 3px;\n    margin-left: 5px;\n    \n}\n    .box{\n    height:90%;\n    margin-top: 0px;\n    margin-left: 5px;\n    \n}\n    .tekst{\n      line-height: 15px;\n      font-size: 2.5vw;\n        top:15px;\n    }\n    .rad{\n        top:10%;\n    }\n}\n\n\n\n",
