          "_customHTML": "<!-- Type valid HTML here #008CBA -->\n\n\n<section class=\"medarbeidere\">\n\n    <div class=\"konteiner\">\n    <div class=\"rad\">\n        <a href=\"Hilde Vale.html\">\n        <div class=\"galleri\" href=\"Hilde Vale.html\">\n            \n            <p class=\"tekst\"><span>Hilde Vale</span><br><br>Stilling: <span class=\"info\">Advokat</span> <br> E-post: vale@valeogkjeldsberg.no<br> Tlf: (+47) 907 45 249</p> \n            <div class=\"img-box\">\n                <img src=\"@PROJECT_PATH@/assets/images/hilde-klippet3.jpg\">\n            \n                <div class=\"box\">\n                    <img src=\"@PROJECT_PATH@/assets/images/hilde-klippet.jpg\">\n                </div>\n            \n            </div>\n            \n        </div>\n        </a>\n        <a href=\"Carl-Fredrik Setsaas.html\">\n        <div class=\"galleri\" href=\"Carl-Fredrik Setsaas.html\">\n            <p class=\"tekst\"> <span>Carl-Fredrik Setsaas</span><br><br>Stilling: Advokat <br> E-post: setsaas@valeco.no<br> Tlf: (+47) 91828738\n   </p> \n        <div class=\"img-box\">\n                <img src=\"@PROJECT_PATH@/assets/images/carl-klippet2.jpg\">\n            \n              <div class=\"box\">\n                    <img src=\"@PROJECT_PATH@/assets/images/carl-klippet2.jpg\">\n\n                </div>\n              \n            </div>\n\n        </div>\n     </a>\n    </div>\n    \n</div>\n\n    \n</section>",
          "_customCSS": "\n\n.medarbeidere {\n  width: 100%;\nheight: 550px;\n    background-color: white;\n}\n\n\n\n.konteiner{\n    position: relative;\n    height: 100%;\n    width: 100%;\n    margin: auto;\n    width: 100%;\n    \n}\n\n.rad{\n    position: absolute;\n    top:50%;\n    left:50%;\n    transform: translate(-50%, -50%);\n    height: 50%;\n    width: 100%;\n}\n\n.galleri{\n    position:relative;\n    height: 100%;\n    width: 600px;\n    background: #aaaaaa;\n    text-align: center;\n    float:left;\n    margin: 0px 70px 0px 70px;\n    border-radius:2.5%;\n    border: 2px solid;\n}\n.img-box{\n    height:90%;\n    margin-top: 27px;\n    margin-left: 5px;\n    border-radius: 2.5%;\n    float:left;\n    \n}\n.box{\n    position:absolute;\n    height:70%;\n    border-radius: 2.5%;\n}\n\n.galleri img{\n    height:100%;\n    border-radius: 2.5%;\n    float:left;  \n}\n\n.overlay{\n    position:relative;\n    opacity: 1;\n    width:100%;\n    height:100%;\n    -webkit-transition: 1s; /* For Safari 3.1 to 6.0 */\n    transition: 1s;\n    border-radius:2.5%;\n    \n}\n\n.tekst{\n    font-family: 'Lora', serif;\n    position: absolute;\n    top:55%;\n    left:75%;\n    transform: translate(-50%, -50%);\n    width: 60%;\n    text-align: left;\n    font-size: 18px;\n    line-height: 40px;\n    opacity: 1;\n    letter-spacing: 2px;\n    z-index:2;\n    //font-weight: bold;\n    font-family: 'Lora';\n    //font-family: 'Great Vibes' ;\n    //font-family: 'Dancing Script' ;\n    font-size: 16px;\n    //border: 2px solid;\n}\n\n.name{\n    font-family: 'Great Vibes' ;\n    padding-left:7%;\n    font-size: 20px;\n}\n\n.box:hover img{\n    opacity: 0;\n    -webkit-transition: .4s; /* For Safari 3.1 to 6.0 */\n    transition: .4s;\n    \n}\n\n\n\n",
